{
  "name": "NativeDemo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "dev": "node scripts/dev.js",
    "ios": "react-native run-ios",
    "test": "jest",
    "storybook:react": "start-storybook -p 9001 -c .storybookReact",
    "storybook:native": "storybook start -p 7007",
    "clean": "rm -rf ios/build/",
    "build": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "build:storybook": "build-storybook -c .storybookReact -o .out",
    "release": "react-native run-ios --configuration Release",
    "zip": "tar -czf /Users/sheacock/Desktop/app.tar.gz /Users/sheacock/Desktop/NativeDemo/ios/build/Build/Products/Release-iphonesimulator/*.app",
    "server": "node server/index.js",
    "myzip": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios"
  },
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.5.2",
    "react-native": "^0.57.3",
    "tslib": "^1.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react": "4.0.0-alpha.3",
    "@storybook/react-native": "^3.4.8",
    "@types/jest": "^23.1.1",
    "@types/react": "^16.4.1",
    "@types/react-native": "^0.57.7",
    "@types/react-test-renderer": "^16.0.1",
    "@types/storybook__react": "^3.0.9",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-jest": "23.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-react-native": "4.0.0",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.1",
    "jest": "23.1.0",
    "prop-types": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "16.3.1",
    "react-native-mock": "^0.3.1",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.3.1",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2",
    "webpack": "^4.22.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
